// HW_18.cpp : 矩陣相乘。

#include "stdafx.h"
#include <stdio.h>
#include <stdlib.h>

int main(int argc, char * argv[]){

	int a[3][3] = { { 7, 8, 9 }, { 8, 9, 7 }, { 9, 7, 8 } }, b[3][3] = { { 2, 0, 0 }, { 0, 2, 0 }, { 0, 0, 2 } }, x[3][3] = { { 0, 0, 0 }, { 0, 0, 0 }, { 0, 0, 0 } }, i, j, k, m;
	printf("Hallo ,JhuKK \n");
	//printf("|7 8 9| \n  A = |8 9 7| \n   |9 7 8| \n\n    |2 0 0| \n   B = |0 2 0| \n   |0 0 2| \n\n   A * B = \n");
	printf("    |7 8 9| \nA = |8 9 7| \n    |9 7 8| \n\n    |2 0 0| \nB = |0 2 0| \n    |0 0 2| \n\nA * B = \n");

		for (i = 0; i < 3; i++){
			printf("| ");
			for (j = 0; j < 3; j++){
				printf(" ");
				for (k = 0; k < 3; k++){
					x[i][j] += a[i][k] * b[k][j];
				}
				printf("%d", x[i][j]);
				for (m = 0; m < 299999999; m++){
					;
				}
			}
			printf(" |\n");
		}
	system("pause");
	return 0;
}

