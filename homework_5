#include <stdio.h>
#include <stdlib.h>
#include<iostream>
// #include "stdafx.h" ※error C3861:'system':找不到識別項(identifier not found)
#include <stdafx.h>

int main()
{

	int num1 = 123, num2 = 456;
	float num3 = 3.14;
	num1 = num2;
	num2 = num3;
	printf("num1=%d\n num2=%d\n", num1, num2);

	system ("PAUSE");
	return 0;
}
