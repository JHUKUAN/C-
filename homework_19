// HW_19.cpp : 限定3x3矩陣，自行輸入值，並做矩陣相乘。

#include "stdafx.h"
#include <stdio.h>
#include <stdlib.h>


int main(){  //主程式用大括號，將整個程式包起來(起頭)

	//int a[3][3] = { { 7, 8, 9 }, { 8, 9, 7 }, { 9, 7, 8 } }, b[3][3] = { { 2, 0, 0 }, { 0, 2, 0 }, { 0, 0, 2 } }, x[3][3] = { { 0, 0, 0 }, { 0, 0, 0 }, { 0, 0, 0 } }, i, j, k, m; //宣告矩陣及變數
	int a[3][3], b[3][3], product[3][3] = { { 0, 0, 0 }, { 0, 0, 0 }, { 0, 0, 0 } }, i, j, k;  //宣告矩陣及變數,並且a與b矩陣不預設值；product[3][3]設定為3x3大小的零矩陣


	//-----------------------輸入a矩陣----------------------------
	//------------------------------------------------------------
	printf("Hallo ,JhuKK，Please input a[3][3] matrix \n"); //印出字串，用" "，較使用者輸入3x3矩陣
	printf("a[3][3]=\n");

	//↓建立3 x 3矩陣
	for (i = 0; i < 3; i++){
		printf(">> ");  //可有可無，美觀用
		for (j = 0; j < 3; j++){
			scanf_s("%d", &a[i][j]);

		}
	}

	printf("\n"); //可有可無，美觀用

	//-----------------------輸入b矩陣----------------------------
	//------------------------------------------------------------
	printf("Hallo ,JhuKK，Please input b[3][3] matrix \n"); //印出字串，用" "，較使用者輸入3x3矩陣
	printf("b[3][3]=\n");

	//↓建立3 x 3矩陣
	for (i = 0; i < 3; i++){
		printf(">> "); //可有可無，美觀用
		for (j = 0; j < 3; j++){
			scanf_s("%d", &b[i][j]);
		}
	}

	printf("\n"); //可有可無，美觀用

	//-----------------------a矩陣 * b矩陣------------------------
	//------------------------------------------------------------
	printf("矩陣相乘結果 \n");
	printf("product[3][3]=\n");
	for (i = 0; i < 3; i++){
		printf("|");  //可有可無，美觀用
		for (j = 0; j < 3; j++){
			for (k = 0; k < 3; k++){
				product[i][j] += a[i][k] * b[k][j];
			}
			printf("%4d", product[i][j]);
		}
		printf("  |\n");  //可有可無，美觀用
	}

	system("pause");
	return 0;

}   //主程式用大括號，將整個程式包起來(結尾)

